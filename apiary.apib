        description: Boolean flag that specifies that "href" property is a URI or
          URI Template. If the property is a URI template, set this value to true.
          By default, this value is false.
        type: boolean
        default: false
      method:
        description: HTTP method for requesting the target of the link.
        type: string
        enum:
        - GET
        - POST
        - PUT
        - DELETE
        - PATCH
        - OPTIONS
        - HEAD
      targetSchema:
        description: Link to the metadata of the resource, such as JSON-schema, that
